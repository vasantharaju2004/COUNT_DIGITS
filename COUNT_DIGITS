class Solution{
public:
    int evenlyDivides(int N){
        int count=0;
        int temp =N;
        while(N>0){
            int ld = N %10; // Last digit 
            
            if( ld != 0  && temp % ld == 0 ){
                 count++;
            } 
            N=N/10;
        }
        return count;
        
    }
};
